{
  "name": "minimon.js",
  "version": "1.0.0",
  "description": "My personal webpage",
  "main": "server/index.js",
  "scripts": {
    "core": "cd core; make",
    "start": "webpack-dev-server --open --mode development",
    "build": "npm run core; webpack --mode production",
    "test": "npm run core; webpack --config webpack.test.config.js; node test/bios.js",
    "deploy": "npm run build; scp public/* node@scyl.us:~/minimon/"
  },
  "keywords": [],
  "author": "Bryon Vandiver",
  "license": "ISC",
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      "@babel/preset-react"
    ]
  },
  "bin": {
    "minimon.js": "app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asterick/minimon.js.git"
  },
  "bugs": {
    "url": "https://github.com/asterick/minimon.js/issues"
  },
  "homepage": "https://github.com/asterick/minimon.js#readme",
  "dependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/preset-react": "^7.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "node-sass": "^4.13.0",
    "normalize.css": "^8.0.1",
    "raw-loader": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-virtualized": "^9.21.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.2",
    "webpack": "^4.41.5"
  },
  "devDependencies": {
    "test": "^0.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
